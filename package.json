{
  "name": "apparel-store",
  "version": "0.0.3",
  "homepage": "https://markhamilton90.github.io/apparel-store",
  "private": true,
  "devDependencies": {
    "concurrently": "6.4.0",
    "gh-pages": "^4.0.0",
    "react-scripts": "4.0.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "firebase": "8.10.0",
    "normalize.css": "^8.0.1",
    "prop-types": "15.7.2",
    "re-base": "4.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0",
    "react-transition-group": "4.4.2",
    "sass": "^1.56.2",
    "serve": "13.0.2"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, sass\" --prefix name \"npm run start\" \"npm run sass\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "now-build": "npm run build && mv build dist",
    "styles:watch": "npm run styles -- -w",
    "sass": "sass --no-source-map --watch src/scss:src/css"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}
